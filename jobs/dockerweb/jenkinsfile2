pipeline {
    agent any
    stages {
        stage('Stage: Hola Mundo') {
            steps {
                echo 'Paso 1. Hola Mundo'
            }
        }
        stage('Stage: Segundo Stage') {
            steps {
                echo 'Paso 2: Hola Dos Veces'
                echo 'Paso 3. Hola Tres Veces'
            }
        }
    }
}
